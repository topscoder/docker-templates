<?xml version="1.0"?>
<Container version="2">
  <Name>laravel</Name>
  <Repository>laravel</Repository>
  <Registry>http://hub.docker.com/bitnami/laravel</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/87798-support-selfhostersnets-template-repository/</Support>
  <Project>https://github.com/laravel/laravel</Project>
  <Overview>Lorem ipsum dolor el samet. Et al.</Overview>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/topscoder/docker-templates/master/templates/laravel.xml</TemplateURL>
  <Icon>https://laravel.com/img/logomark.min.svg</Icon>
  <Config Name="WebUI Port" Target="8080" Default="8080" Mode="tcp" Description="Container Port: 8080" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="Theme" Target="ADMINER_DESIGN" Default="flat" Description="Container Variable: ADMINER_DESIGN" Type="Variable" Display="always" Required="false" Mask="false"/>
</Container>
